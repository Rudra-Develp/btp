<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core">
   <Dialog
      id="helloDialog"
      title="Dialog">
   <content>
      <core:Icon id="_IDGenIcon1"
         src="sap-icon://user-edit"
         size="2rem"
         class="sapUiMediumMargin "/>
      <VBox id="_IDGenVBox" class="sapUiMediumMargin">
	      <FormattedText id="_IDGenFormattedText" htmlText="{device>/HTML}"/>
	   </VBox>
   </content>
       
 	<beginButton>
         <Button id="_IDGenButton8"
            text="okay"
            press=".onOkayclick"/>
   </beginButton>
      
   <endButton>
         <Button id="_IDGenButton9"
            text="cancel"
            press=".onCloseDialog" />
   </endButton>

</Dialog>

</core:FragmentDefinition>